<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.iclub21" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".http.MyAppliction"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".util.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".util.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity android:name=".util.SelectedCityActivity" />
        <activity android:name=".util.RegisterActivity" />
        <activity
            android:name=".util.RoleActivity"
            android:label="@string/title_activity_role" >
        </activity>
        <activity
            android:name=".util.RoleTalentsActivity"
            android:label="@string/title_activity_role_talents" >
        </activity>
        <activity
            android:name=".util.ResumeActivity"
            android:label="@string/title_activity_resume" >
        </activity>
    </application>

</manifest>
